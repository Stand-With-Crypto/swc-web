    "@thirdweb-dev/react": "^4.1.15",
    "@types/lodash": "^4.14.202",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.3.1",
    "ethers": "^5.7.2",
    "formatjs": "^0.1.1",
    "graphql": "^16.8.1",
    "install": "^0.13.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
